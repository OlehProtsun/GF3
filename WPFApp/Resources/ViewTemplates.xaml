<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"

                    xmlns:vm="clr-namespace:WPFApp.ViewModel"

                    xmlns:evm="clr-namespace:WPFApp.ViewModel.Employee"
                    xmlns:svm="clr-namespace:WPFApp.ViewModel.Shop"
                    xmlns:dvm="clr-namespace:WPFApp.ViewModel.Database"
                    xmlns:hvm="clr-namespace:WPFApp.ViewModel.Home"

                    xmlns:avmMain="clr-namespace:WPFApp.ViewModel.Availability.Main"
                    xmlns:avmList="clr-namespace:WPFApp.ViewModel.Availability.List"
                    xmlns:avmEdit="clr-namespace:WPFApp.ViewModel.Availability.Edit"
                    xmlns:avmProfile="clr-namespace:WPFApp.ViewModel.Availability.Profile"

                    xmlns:cvmEdit="clr-namespace:WPFApp.ViewModel.Container.Edit"
                    xmlns:cvmList="clr-namespace:WPFApp.ViewModel.Container.List"
                    xmlns:cvmProfile="clr-namespace:WPFApp.ViewModel.Container.Profile"
                    xmlns:cvmSchEdit="clr-namespace:WPFApp.ViewModel.Container.ScheduleEdit"
                    xmlns:cvmSchProfile="clr-namespace:WPFApp.ViewModel.Container.ScheduleProfile"
                    xmlns:cvmSchList="clr-namespace:WPFApp.ViewModel.Container.ScheduleList"

                    xmlns:v="clr-namespace:WPFApp.View"
                    xmlns:av="clr-namespace:WPFApp.View.Availability"
                    xmlns:cv="clr-namespace:WPFApp.View.Container"
                    xmlns:ev="clr-namespace:WPFApp.View.Employee"
                    xmlns:sv="clr-namespace:WPFApp.View.Shop"
                    xmlns:hv="clr-namespace:WPFApp.View.Home">

    <!-- Home module -->
    <DataTemplate DataType="{x:Type hvm:HomeViewModel}">
        <hv:HomeView />
    </DataTemplate>

    <!-- Employee module -->
    <DataTemplate DataType="{x:Type evm:EmployeeViewModel}">
        <ev:EmployeeView />
    </DataTemplate>

    <!-- Shop module -->
    <DataTemplate DataType="{x:Type svm:ShopViewModel}">
        <sv:ShopView />
    </DataTemplate>

    <!-- Availability module -->
    <DataTemplate DataType="{x:Type avmMain:AvailabilityViewModel}">
        <av:AvailabilityView />
    </DataTemplate>

    <!-- Container module -->
    <DataTemplate DataType="{x:Type cvmEdit:ContainerViewModel}">
        <v:ContainerView />
    </DataTemplate>

    <!-- Database module -->
    <DataTemplate DataType="{x:Type dvm:DatabaseViewModel}">
        <v:DatabaseView />
    </DataTemplate>

    <!-- Employee -->
    <DataTemplate DataType="{x:Type evm:EmployeeListViewModel}">
        <ev:EmployeeListView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type evm:EmployeeEditViewModel}">
        <ev:EmployeeEditView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type evm:EmployeeProfileViewModel}">
        <ev:EmployeeProfileView />
    </DataTemplate>

    <!-- Shop -->
    <DataTemplate DataType="{x:Type svm:ShopListViewModel}">
        <sv:ShopListView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type svm:ShopEditViewModel}">
        <sv:ShopEditView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type svm:ShopProfileViewModel}">
        <sv:ShopProfileView />
    </DataTemplate>

    <!-- Availability (list/edit/profile) -->
    <DataTemplate DataType="{x:Type avmList:AvailabilityListViewModel}">
        <av:AvailabilityListView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type avmEdit:AvailabilityEditViewModel}">
        <av:AvailabilityEditView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type avmProfile:AvailabilityProfileViewModel}">
        <av:AvailabilityProfileView />
    </DataTemplate>

    <!-- Container (list/edit/profile) -->
    <DataTemplate DataType="{x:Type cvmList:ContainerListViewModel}">
        <cv:ContainerListView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvmEdit:ContainerEditViewModel}">
        <cv:ContainerEditView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvmProfile:ContainerProfileViewModel}">
        <cv:ContainerProfileView />
    </DataTemplate>

    <!-- Schedule (edit/profile) -->
    <DataTemplate DataType="{x:Type cvmSchEdit:ContainerScheduleEditViewModel}">
        <cv:ContainerScheduleEditView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvmSchProfile:ContainerScheduleProfileViewModel}">
        <cv:ContainerScheduleProfileView />
    </DataTemplate>
</ResourceDictionary>
